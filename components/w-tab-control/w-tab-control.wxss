/* components/w-tab-control/w-tab-control.wxss */
.w-tab-control {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  background-color: white;
  padding-bottom: 12rpx;
}
.tab-control {
  flex: 1;
  text-align: center;
}
.active{
  color: #ff5777;
}
.active .item{
  padding: 20rpx 16rpx;
  border-bottom: 6rpx solid red;
}